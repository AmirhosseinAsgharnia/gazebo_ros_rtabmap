        <model name="QR_0">
            <link name="QR_0">
                <visual name="QR_0">

                <pose>1.5 4 0.5 0 0 0</pose>

                    <geometry>
                        <plane>
                        <normal>0 1 0</normal> <!-- Facing Z+ -->
                        <size>0.2 0.2</size>   <!-- Adjust as needed -->
                        </plane>
                    </geometry>

                    <material>
                        <double_sided>true</double_sided>
                        <ambient>1 1 1 1</ambient>
                        <diffuse>0.8 0.8 0.8 1</diffuse>
                        <specular>1 0.8 0.8 1</specular>
